<?xml version="1.0" encoding="utf-8"?>
<appengine-web-app xmlns="http://appengine.google.com/ns/1.0">
	<application>stk-gae</application>
	<version>develop</version>

	<resource-files>
		<include path="/**.xml" />
		<include path="/**.jsp" />
		<include path="/**.properties" />
		<include path="/**.csv" />
		<include path="/**.tld" />
	</resource-files>

	<static-files>
		<include path="/**.png" />
		<include path="/**.gif" />
		<include path="/**.css" />
		<include path="/**.js" />
	</static-files>

	<sessions-enabled>true</sessions-enabled>

	<inbound-services>
		<service>mail</service>
	</inbound-services>

	<system-properties>
		<property name="java.util.logging.config.file" value="WEB-INF/classes/logging.properties" />
	</system-properties>
</appengine-web-app>
